from numpy import random

#freqs = {'the':3,'gerbil':2,'Edgar':1}

def printer(wordSeqs):
    sum = 0 #sum is sum of values in dict
    for value in wordSeqs.values():
        sum = sum + value
    randRange = range(0,sum+1)
    rando = random.choice(randRange)
    
    #start with the first k,v pair. If rando - the v is less than 0, return
    #the key. Otherwise, move to the next k,v, pair and subtract the v from
    #rando again
    for word, frequency in wordSeqs.items():
        rando = rando - frequency
        #print(rando)
        if rando < 0:
            print(word)
            break
        else:
            continue
#printer(freqs)


def tenWords():
    counter = 0
    while counter < 10:
        printer(wordSeqs)
        counter += 1
    
tenWords()
